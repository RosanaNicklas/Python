    "import numpy as np\n",
    "numero = int(input(\"Introduce un numero: \"))\n",
    "def factorial(numero):\n",
    "    listita = np.arange(1, numero + 1)\n",
    "    producto = 1\n",
    "    for i in listita:\n",
    "        producto = producto * i\n",
    "    return producto\n",
    "factorial(numero)\n",
    "        "
